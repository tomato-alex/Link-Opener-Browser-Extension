{
  "name": "Lobster",
  "version": "1.5",
  "description": "Import Tabs from File or Export to File",
  "icons": {
    "16": "icons/lobster_icon16.png",
    "48": "icons/lobster_icon16.png",
    "128": "icons/lobster_icon128.png"
  },
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}