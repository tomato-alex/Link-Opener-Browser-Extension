{
    "manifest_version": 3,
    "permissions": ["tabs", "tabGroups", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
