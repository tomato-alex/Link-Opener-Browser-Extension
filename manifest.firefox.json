{
    "manifest_version": 2,
    "name": "Tab Manager",
    "version": "1.2",
    "description": "Import Tabs from File or Export to File",
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
